                    cellspacing="0"
                  >
                  <thead>
                    <tr>
                        <th class="text-center">Asset Name</th>
                        <th class="text-center">Purchase</th>
                      <th class="text-center">Rent Amount</th>
                      <th class="text-center">Expenses</th>
                      <th class="text-center">Total profit</th>
                      <th></th>
                    </tr>
                  </thead>
                  <tfoot>
                    <tr>
                      <th class="text-center">Asset Name</th>
                      <th class="text-center">Purchase</th>
                      <th class="text-center">Rent Amount</th>
                      <th class="text-center">Expenses</th>
                      <th class="text-center">Total profit</th>
                      <th></th>
                    </tr>
                  </tfoot>
                  <tbody> 

                    <% for( let asset of Assets ) { %>

                        <tr>
                            <td class="text-center"><%= asset.name %></td>
                            <td class="text-center">#<%= asset.current_amount %></td>
                            <td class="text-center">#<%= asset.amount %></td>
                            <td class="text-center">#<%= !(asset.expense)? '0' : asset.expense %></td>
                            <td class="text-primary text-center">#<%= asset.amount-asset.current_amount-asset.expense %></td>
                            <td title="Edit expense" class="icon text-center" onclick="
                            $('#FormEdit').attr('action', '/admin/depreciate/update/<%= asset.id %>');
                            $('#expense').val('<%=asset.expense %>');
                            ">                       
                            <i class="fa fa-pen-to-square" data-bs-toggle="modal" data-bs-target="#editModal"></i>
                        </tr>
                       
                      <% } %>
                      <div class="modal fade " id="editModal" tabindex="-1" role="dialog"
                        aria-labelledby="exampleModalLabel" aria-hidden="true">
                        <div class="modal-dialog" role="document">
                          <div class="modal-content">
                            <div class="modal-header" style=" background-color: purple; color:aliceblue">
                              <h5 class="modal-title" id="exampleModalLabel">Edit expense </h5>
                              <button class="close" type="button" data-bs-dismiss="modal" aria-label="Close">
                                <span aria-hidden="true" class="text-white">×</span>
                              </button>
                            </div>
                            <div class="modal-body">
                              <div>
                              <form method="POST" id="FormEdit">
                                <div class="mb-3">
                                  <label for="" class="form-label">expenses</label>
                                  <input type="text"
                                    class="form-control" name="expense" id="expense"  aria-describedby="helpId" placeholder="">
                                </div>
                                <div class="mx-auto">
                                <center><button class="btn-primary mx-auto" type="submit" >save</button></center>
                                </div>
                              </form>
                              </div>
                              </div>
                            <div class="modal-footer" style="background-color: purple;">
                            </div>
                          </div>
                        </div>
                      </div>
                   
                    </tbody>
                  </table>
